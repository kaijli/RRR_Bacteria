@article{pan_engineered_2022,
	title = {Engineered {Bacteria} for {Enhanced} {Radiotherapy} against {Breast} {Carcinoma}},
	volume = {16},
	issn = {1936-0851},
	url = {https://doi.org/10.1021/acsnano.1c08350},
	doi = {10.1021/acsnano.1c08350},
	abstract = {Radiotherapy is widely applied for multiple malignant tumors ablation in the clinic. However, redundant doses of X-rays might destroy normal tissue in the periphery of tumor sites. Here, we developed an integrated nanosystem (Bac@BNP) composed of engineered bacteria (Bac) and Bi2S3 nanoparticles (BNPs) for sensitizing radiotherapy. Bac could target and colonize in tumor sites alternatively, which overexpressed cytolysin A (ClyA) protein to regulate the cell cycle from a radioresistant phase to a radiosensitive phase. Simultaneously, peptide-modified BNPs, as a radiosensitizer with a high-Z element, was released from the surface of Bac owing to the matrix metalloproteinase-2 (MMP-2) response in the tumor microenvironment. Under X-ray irradiation, BNPs could enhance the radiotherapy sensitivity by triggering the intracellular generation of reactive oxygen species (ROS), coupled with DNA damage. In this constructed nanosystem, the combination of Bac@BNP and X-ray irradiation led to significant suppression of breast carcinoma in murine models with reduced side effects.},
	number = {1},
	urldate = {2022-04-15},
	journal = {ACS Nano},
	author = {Pan, Pei and Dong, Xue and Chen, Ying and Zeng, Xuan and Zhang, Xian-Zheng},
	month = jan,
	year = {2022},
	note = {Publisher: American Chemical Society},
	pages = {801--812},
	file = {ACS Full Text Snapshot:/Users/kaitlynli/Zotero/storage/9XLPKUPA/acsnano.html:text/html;Full Text PDF:/Users/kaitlynli/Zotero/storage/W6GPUSPA/Pan et al. - 2022 - Engineered Bacteria for Enhanced Radiotherapy agai.pdf:application/pdf},
}

@article{liu_high_2022,
	title = {High {Proportions} of {Radiation}-{Resistant} {Strains} in {Culturable} {Bacteria} from the {Taklimakan} {Desert}},
	volume = {11},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2079-7737},
	url = {https://www.mdpi.com/2079-7737/11/4/501},
	doi = {10.3390/biology11040501},
	abstract = {The Taklimakan Desert located in China is the second-largest shifting sand desert in the world and is known for its harsh conditions. Types of γ-rays or UV radiation-resistant bacterial strains have been isolated from this desert. However, there is no information regarding the proportions of the radiation-resistant strains in the total culturable microbes. We isolated 352 bacterial strains from nine sites across the Taklimakan Desert from north to south. They belong to Actinobacteria, Firmicutes, Proteobacteria, and Bacteroidetes. The phylum Actinobacteria was the most predominant in abundance and Firmicutes had the highest species richness. Bacteroidetes had the lowest abundance and was found in four sites only, while the other three phyla were found in every site but with different distribution profiles. After irradiating with 1000 J/m2 and 6000 J/m2 UV-C, the strains with survival rates higher than 10\% occupied 72.3\% and 36.9\% of all culturable bacteria, respectively. The members from Proteobacteria had the highest proportions, with survival rates higher than 10\%. After radiation with 10 kGy γ-rays, Kocuria sp. TKL1057 and Planococcus sp. TKL1152 showed higher radiation-resistant capabilities than Deinococcus radiodurans R1. Besides obtaining several radiation-resistant extremophiles, this study measured the proportions of the radiation-resistant strains in the total culturable microbes for the first time. This study may help to better understand the origin of radioresistance, especially by quantitatively comparing proportions of radiation-resistant extremophiles from different environments in the future.},
	language = {en},
	number = {4},
	urldate = {2022-04-15},
	journal = {Biology},
	author = {Liu, Yang and Chen, Tuo and Li, Juan and Wu, Minghui and Liu, Guangxiu and Zhang, Wei and Zhang, Binglin and Zhang, Songlin and Zhang, Gaosen},
	month = apr,
	year = {2022},
	note = {Number: 4
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {culturable bacteria, radiation-resistant extremophiles, Taklimakan desert, UV-C, γ-rays},
	pages = {501},
	file = {Full Text PDF:/Users/kaitlynli/Zotero/storage/5KXDSGJY/Liu et al. - 2022 - High Proportions of Radiation-Resistant Strains in.pdf:application/pdf;Snapshot:/Users/kaitlynli/Zotero/storage/UNASJMH9/htm.html:text/html},
}


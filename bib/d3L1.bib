Place: Basel, Switzerland
Publisher: MDPI AG},
	keywords = {DNA damage, DNA repair, Radiation, Bacteria, Biology, Deoxyribonucleic acid--DNA, Genomes, Genomic instability, Lesions, Lethality, Microorganisms, Proteins, Radiation damage, Radiation effects, Radiation tolerance, Stability, Toxicity},
	pages = {26},
